#ifndef CARD_H_
#define CARD_H_
#include <string>
#include <iostream>

class Card {
private:
	enum cardType {Infantry, Artillery, Cavalry};
	cardType val;
	std::string territory;
public:
	Card();
	Card(cardType, std::string);
	cardType getCardVal();
	virtual ~Card();
};

#endif /* CARD_H_ */
